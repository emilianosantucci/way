{
  "name": "core-feature-application",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/core/feature/application",
  "tags": [],
  "targets": {
    "test": {
      "executor": "@nx-go/nx-go:test"
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    },
    "tidy": {
      "executor": "@nx-go/nx-go:tidy"
    },
    "convert": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "commands": [
          "goverter gen './api/dto/... ./service/model/... ./repository/entity/...'"
        ],
        "parallel": false
      }
    }
  }
}
